<section class="content">
      <!-- Mobile Menu -->
      <div class="row">
          <div id="category-header" class="category-header col-xs-12 visible-xs">
                 <h1>
                           <!-- Dropdown  -->
                            <div id="drop-down" class="drop-down">
                                <a href="#">
                                  Parenting 
                                  <i>
                                    +
                                  </i>
                                </a>
                                <ul class="hide">
                                    <li>
                                      <a href="#">Pregnancy</a>
                                    </li>
                                    <li>
                                      <a href="#">Baby</a>
                                    </li>
                                    <li>
                                      <a href="#">Toddlers &amp; Preschoolers</a>
                                    </li>
                                    <li>
                                      <a href="#">Big Kid</a>
                                    </li>
                                    <li>
                                      <a href="#">Tweens &amp; Teens</a>
                                    </li>
                                    <li>
                                      <a href="#">Being a Mom</a>
                                    </li>
                                    <li>
                                      <a href="#">Parenting News</a>
                                    </li>
                                </ul>
                              </div>
                              <!-- Dropdown -->
                          </h1>
            </div>
        </div>
        <!-- End of mobile Menu -->

      
      <!-- Row One -->
      <div class="row articles-block top-articles-home">
           {{#slice slotOne 0 3}}
              <div class="col-lg-4 col-md-4 col-sm-4">
                  {{>  cards/cardTemplate }}
              </div>
            {{/slice}}
      </div>

       <!-- Row 2,3,4  -->
          <div class="col-lg-8 col-md-8 col-sm-8 col-xs-10 col-md-offset-0 col-lg-offset-0 col-sm-offset-0 col-xs-offset-1">
            
            <!-- visible-lg visible-md visible-sm -->
            <div class="col-lg-6 col-md-6 col-sm-6 visible-md visible-lg visible-lg">
                  {{>  header/menuCategory menu=menu}}
            </div>

            <!-- 2 x 2 -->
              {{#slice slotTwo 0 1}}
                <div class="col-lg-6 col-md-6 col-sm-6">
                  {{>  cards/cardTemplateCategory regular='regular'}}
                </div>
              {{/slice}}
              
                
               {{#slice slotTwo 1 3}}
                <div class="col-lg-6 col-md-6 col-sm-6">
                  {{>  cards/cardTemplateCategory regular='regular'}}
                </div>
              {{/slice}}
              
            <!-- end of 2 x 2 -->
            
              <!-- 1 X 1 Big Image  -->
              <div class="col-lg-12 col-md-12 col-sm-12">
                {{#each editorial}}
                    {{> cards/cardTemplateCategory}}
                {{/each}}
              </div>
            
            <!-- end of 1 x 1 Big Image -->
          </div>
          
          <!-- Sidebar Content -->
          <aside>
              <div class="col-lg-4 col-md-4 col-sm-4 col-xs-10 col-md-offset-0 col-lg-offset-0 col-sm-offset-0 col-xs-offset-1">
                    <!-- Widget for Slot One -->
                    {{#slotWidgetMapper 'slot_3'}}
                      {{> widgets/widgetGeneric}}
                    {{/slotWidgetMapper }}
                    <!-- Widget For Slot Two -->
                    {{> widgets/trending  trending=slotTwo}}
                    <!-- Widget for Slot Four -->
                    {{#slotWidgetMapper 'slot_4'}}
                      {{> widgets/widgetGeneric}}
                    {{/slotWidgetMapper }}
                    <!-- Widget For Slot Two -->
               </div>
          </aside>
         <!-- End of Sidebar -->


        <!-- Row 2,3,4  -->
        <div class="col-lg-8 col-md-8 col-sm-8 col-xs-10 col-md-offset-0 col-lg-offset-0 col-sm-offset-0 col-xs-offset-1">
            
             <!-- 2 x 2 -->
            {{#each slotTwo}}
              <div class="col-lg-6 col-md-6 col-sm-6">
                  {{>  cards/cardTemplateCategory regular='regular'}}
              </div>
            {{/each}}
            
            <!-- 1 X 1 Big Image  -->
            <div class="col-lg-12 col-md-12 col-sm-12">
              {{#each editorial}}
                  {{>  cards/cardTemplateCategory}}
              {{/each}}
              
            </div>
            <!-- end of 1 x 1 Big Image -->

            <!-- end of 2 x 2 -->
        </div>

</section>
<div class="clearfix"></div>
{{> commons/pager }}
 